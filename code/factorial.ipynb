      "source": [
        "fn factorial(n:Int) -> Int:\n",
        "  if n < 0:\n",
        "    print(\"Factorial is not defined for negative numbers\")\n",
        "  if n == 0:\n",
        "    return 1\n",
        "  else:\n",
        "    return n * factorial(n-1)\n",
        "\n",
        "fn main():\n",
        "  let number: Int = 5\n",
        "  let result: Int = factorial(number)\n",
        "  print(result)\n"
      ]
    }
  ]
}